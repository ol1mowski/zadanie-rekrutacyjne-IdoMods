{
  "name": "idosell-orders-api",
  "version": "1.0.0",
  "description": "API do pobierania zamówień z idoSell",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug"
  },
  "keywords": [
    "idosell",
    "api",
    "orders"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.32",
    "@types/node-schedule": "^2.1.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^3.1.2"
  }
